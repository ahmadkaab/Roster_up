{
  "expo": {
    "name": "Roster_up",
    "slug": "Roster_up",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/rosterup-icon.png",
    "scheme": "rosterup",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#111827",
        "foregroundImage": "./assets/images/rosterup-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ahmadkaab.Roster_up"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/rosterup-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/rosterup-splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#111827",
          "dark": {
            "backgroundColor": "#111827"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "11fd0e2e-42e8-4b9f-8ebc-739d12844919"
      }
    }
  }
}
